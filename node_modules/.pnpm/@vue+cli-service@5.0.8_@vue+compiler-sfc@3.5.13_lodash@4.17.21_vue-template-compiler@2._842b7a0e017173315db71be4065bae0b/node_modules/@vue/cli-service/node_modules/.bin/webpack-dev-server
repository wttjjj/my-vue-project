#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wangtingting/Downloads/web_project/my-vue-project/node_modules/.pnpm/webpack-dev-server@4.15.2_debug@4.4.0_webpack@5.99.5/node_modules/webpack-dev-server/bin/node_modules:/Users/wangtingting/Downloads/web_project/my-vue-project/node_modules/.pnpm/webpack-dev-server@4.15.2_debug@4.4.0_webpack@5.99.5/node_modules/webpack-dev-server/node_modules:/Users/wangtingting/Downloads/web_project/my-vue-project/node_modules/.pnpm/webpack-dev-server@4.15.2_debug@4.4.0_webpack@5.99.5/node_modules:/Users/wangtingting/Downloads/web_project/my-vue-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wangtingting/Downloads/web_project/my-vue-project/node_modules/.pnpm/webpack-dev-server@4.15.2_debug@4.4.0_webpack@5.99.5/node_modules/webpack-dev-server/bin/node_modules:/Users/wangtingting/Downloads/web_project/my-vue-project/node_modules/.pnpm/webpack-dev-server@4.15.2_debug@4.4.0_webpack@5.99.5/node_modules/webpack-dev-server/node_modules:/Users/wangtingting/Downloads/web_project/my-vue-project/node_modules/.pnpm/webpack-dev-server@4.15.2_debug@4.4.0_webpack@5.99.5/node_modules:/Users/wangtingting/Downloads/web_project/my-vue-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../webpack-dev-server@4.15.2_debug@4.4.0_webpack@5.99.5/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../../../../webpack-dev-server@4.15.2_debug@4.4.0_webpack@5.99.5/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
